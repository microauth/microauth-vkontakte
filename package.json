{
  "name": "microauth-vkontakte",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "Vkontakte OAuth for micro.",
  "author": "Cody Brunner <rockchalkwushock@icloud.com> (https://rcws-development.com)",
  "homepage": "https://github.com/microauth/microauth-vkontakte#readme",
  "repository": "https://github.com/microauth/microauth-vkontakte.git",
  "bugs": "https://github.com/microauth/microauth-vkontakte/issues",
  "main": "lib/index.js",
  "files": [
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "micro",
    "oauth2",
    "vkontakte"
  ],
  "scripts": {
    "commit": "nps commit",
    "precommit": "lint-staged && nps validate",
    "prettier": "prettier --single-quote --no-semi --write",
    "start": "nps",
    "test": "nps test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "peerDependencies": {
    "micro": ">=7.3.0"
  },
  "dependencies": {
    "micro-redirect": "~1.0.0",
    "request": "~2.83.0",
    "request-promise": "~4.2.0",
    "uuid": "~3.1.0"
  },
  "devDependencies": {
    "all-contributors-cli": "~4.5.0",
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "codecov": "~2.3.0",
    "commitizen": "~2.9.6",
    "condition-circle": "~1.5.0",
    "cz-conventional-changelog": "~2.0.0",
    "eslint": "~4.7.2",
    "eslint-config-equimper": "~2.2.1",
    "eslint-config-prettier": "~2.6.0",
    "husky": "~0.14.3",
    "jest": "~21.2.0",
    "lint-staged": "~4.2.3",
    "nps": "~5.7.1",
    "nps-utils": "~1.3.0",
    "nsp": "~3.0.0",
    "prettier": "~1.7.0",
    "semantic-release": "~8.0.3"
  }
}
